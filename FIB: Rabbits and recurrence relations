#start with 1 pair of rabbits in each first and second months (given)
x = [1,1]

def wascally(months, pairs_per_month):
    counter = 0
    while counter+2 < months:
        new_val = ((x[-2]*pairs_per_month)+x[-1])
        x.append(new_val)
        counter += 1
    print(wascally[-1])
    
wascally(29,4) #returns 170361678269
